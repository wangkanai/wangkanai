<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>ASP.NET Core Detection Device resolver components</Description>
    <Copyright>Copyright © 2018 Sarin Na Wangkanai</Copyright>
    <Authors>Sarin Na Wangkanai</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>Wangkanai.Detection.Device</AssemblyName>
    <PackageId>Wangkanai.Detection.Device</PackageId>
    <PackageTags>aspnetcore;detection;device</PackageTags>
    <PackageIconUrl>http://0.gravatar.com/avatar/f58457d38ab3bc43526c6e52af17494e</PackageIconUrl>
    <PackageProjectUrl>https://github.com/wangkanai/Detection</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/wangkanai/Detection/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/wangkanai/Detection</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <VersionPrefix>2.0.0</VersionPrefix>
    <VersionSuffix>beta11</VersionSuffix>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RootNamespace>Wangkanai.Detection</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Wangkanai.Detection.Core\Wangkanai.Detection.Core.csproj" />
  </ItemGroup>

</Project>
