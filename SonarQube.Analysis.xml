<SonarQubeAnalysisProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                             xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
    <!-- Mandatory-->
    <Property Name="sonar.host.url">https://sonarcloud.io</Property>
    <!-- Generic  -->
    <Property Name="sonar.branch.name">main</Property>
    <Property Name="sonar.sourceEncoding">UTF-8</Property>
    <!-- <Property Name="sonar.project.monorepo.enabled">true</Property> -->
    <!-- Source   -->
    <!-- <Property Name="sonar.sources">.</Property> -->
    <!--    <Property Name="sonar.inclusions"></Property>-->
    <Property Name="sonar.exclusions">Assets/, **/tests/**, **/benchmark/**, **/node_modules/**, **/lib/**,
        **/BenchmarkDotNet.Artifacts/**
    </Property>
    <!-- Tests    -->
    <!--    <Property Name="sonar.tests">.</Property>-->
    <!--    <Property Name="sonar.tests.inclusions">**/tests/**</Property>-->
    <!--    <Property Name="sonar.tests.exclusions">Assets/</Property>-->
    <!-- Coverage -->
    <Property Name="sonar.cs.vscoveragexml.reportsPaths">coverage.xml</Property>
    <Property Name="sonar.coverage.exclusions">**/demo/**, **/samples/**, **/hosts/**
    </Property>
    <!-- Duplicate-->
    <!--    <Property Name="sonar.cpd.exclusions"></Property>-->
</SonarQubeAnalysisProperties>